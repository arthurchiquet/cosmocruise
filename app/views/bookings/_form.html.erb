<%= simple_form_for [@trip, @booking] do |f| %>
<%= f.input :start_date,
            as: :string,
            input_html: { data: { controller: "datepicker" } } %>
<%= f.input :end_date,
            as: :string,
            input_html: { data: { controller: "datepicker" } } %>
<%= f.submit class: "btn btn-outline-light" %>
<% end %>
